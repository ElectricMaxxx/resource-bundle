<?xml version="1.0" encoding="UTF-8"?>
<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="cmf_resource.registry.container.class"></parameter>
    </parameters>

    <services>

        <service id="cmf_resource.finder.selector.parser" class="Symfony\Cmf\Component\Resource\Finder\SelectorParser" />

        <service id="cmf_resource.registry" class="Symfony\Cmf\Bundle\ResourceBundle\Registry\RepositoryRegistry">
            <argument /> <!-- Factories service IDs -->
            <argument>%cmf_resource.repository_configuration%</argument>
        </service>

    </services>
</container>
